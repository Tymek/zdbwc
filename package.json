	"scripts": {
		"next": "cross-env NEXT_TELEMETRY_DISABLED=1 next",
		"build": "npm run next -- build",
		"analyze": "npm run clean && cross-env ANALYZE=true npm run build",
		"start": "npm run next -- start",
		"dev": "cross-env NEXT_TELEMETRY_DISABLED=1 NODE_OPTIONS='--inspect=0.0.0.0:9229' next dev",
		"dev:test": "cross-env NODE_ENV=test BABEL_ENV=test jest . -o --watch",
		"dev:fix": "npm run test:eslint -- --fix",
		"clean": "rimraf .next public/generated",
		"backend:migrate": "hasura migrate apply",
		"backend:seeds": "hasura seeds apply",
		"backend:generate": "graphql-codegen --config codegen.js",
		"backend": "run-s backend:*",
		"console": "hasura console",
		"test:eslint": "eslint . --ext .js,.jsx,.ts,.tsx,.json --ignore-path .gitignore",
		"test:jest": "cross-env NODE_ENV=test BABEL_ENV=test jest . --ci --collectCoverage",
		"test:depcheck": "depcheck .",
		"test": "run-s test:*"
	},
